## *updated nov-8*
# Local environment variables for DinnerDecider
# Replace with your Google Maps Places API key (keep this file private)

# Optional fallback for local dev server port
PORT=3003

NEXT_PUBLIC_GOOGLE_API_KEY=your_google_places_key         # Google Maps / Places API

NEXT_PUBLIC_BUTTONDOWN_TOKEN=optional_buttondown_token     # Optional (for newsletter or email signups)

NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# === SERVER-SIDE (never expose publicly) ===
OPENAI_API_KEY=your_openai_api_key
OPENWEATHER_API_KEY=your_openweather_api_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_URL=https://your-project.supabase.co        # same base URL as above
SUPABASE_ANON_KEY=your_supabase_anon_key    # duplicate for Node access
SUPABASE_DB_URL=
SUPABASE_DB_PASSWORD=your_db_password
SUPABASE_JWT_SECRET=your_supabase_jwt_secret
SUPABASE_ACCESS_TOKEN=your_supabase_access_token


# Stripe Configuration
STRIPE_PUBLIC_KEY=your_stripe_public_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Product Prices
STRIPE_MONTHLY_PRICE_ID=your_stripe_monthly_price_id
STRIPE_ANNUAL_PRICE_ID=your_stripe_annual_price_id

NEXT_PUBLIC_DEV_ORIGIN=http://localhost:3003
